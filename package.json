{
  "name": "foldeflux-app",
  "productName": "FolderFlux",
  "version": "1.5.0",
  "description": "FolderFlux - Organização inteligente",
  "main": "./out/main/index.js",
  "author": "André Ruan <ruan.fullstack@gmail.com>",
  "homepage": "https://folderflux.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ruan-andre/folderflux.git"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "electron-vite preview",
    "icons:build": "node scripts/build-icons.mjs",
    "predev": "pnpm icons:build",
    "dev": "electron-vite dev --watch",
    "prebuild": "pnpm icons:build",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "pnpm run build && electron-builder --linux",
    "publish:win": "pnpm build:win --publish always",
    "publish:linux": "pnpm build:linux --publish always",
    "migrate:generate": "drizzle-kit generate --config drizzle.config.ts"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@iconify-json/eva": "^1.2.3",
    "@iconify-json/fluent-color": "^1.2.18",
    "@iconify-json/fluent-emoji-flat": "^1.2.5",
    "@iconify-json/ic": "^1.2.4",
    "@iconify-json/icon-park": "^1.2.4",
    "@iconify-json/line-md": "^1.2.11",
    "@iconify-json/logos": "^1.2.9",
    "@iconify-json/material-icon-theme": "^1.2.29",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/noto": "^1.2.7",
    "@iconify-json/noto-v1": "^1.2.5",
    "@iconify-json/simple-icons": "^1.2.52",
    "@iconify-json/streamline-ultimate-color": "^1.2.2",
    "@iconify-json/vscode-icons": "^1.2.30",
    "@iconify/react": "^5.2.1",
    "@mui/material": "^7.0.2",
    "auto-launch": "^5.0.6",
    "better-sqlite3": "^11.9.1",
    "chokidar": "^4.0.3",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.43.1",
    "electron-log": "^5.4.3",
    "electron-store": "^7.0.3",
    "electron-updater": "^6.6.2",
    "fast-deep-equal": "^3.1.3",
    "immer": "^10.1.1",
    "lodash.clonedeep": "^4.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.3",
    "shepherd.js": "^14.5.1",
    "use-immer": "^0.11.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-static-copy": "^2.3.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@iconify/types": "^2.0.0",
    "@types/auto-launch": "^5.0.5",
    "@types/better-sqlite3": "^7.6.13",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "drizzle-kit": "^0.31.1",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.4.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^6.2.6"
  }
}
